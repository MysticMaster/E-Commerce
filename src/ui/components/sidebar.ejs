<div class="top">
    <i class="uil uil-bars sidebar-toggle"></i>

    <button
            id="dropdownUserAvatarButton"
            data-dropdown-toggle="dropdownAvatar"
            class="flex text-sm bg-gray-800 rounded-full"
            type="button"
    >
        <span class="sr-only">Open user menu</span>
        <img
                style="width: 35px;  height: 35px"
                class="w-5 h-5 rounded-full"
                src="data:image/jpeg;base64,"
                alt="user photo"
        />
    </button>

    <!-- Dropdown menu -->
    <div
            id="dropdownAvatar"
            class="z-10 hidden bg-white divide-y divide-gray-100 rounded-lg shadow w-50 dark:bg-gray-700 dark:divide-gray-600"
    >
        <div class="px-4 py-3 text-sm text-gray-900 dark:text-white">
            <div>Bonnie Green</div>
            <div class="font-medium truncate">name@flowbite.com</div>
        </div>
        <ul
                class="py-2 text-sm text-gray-700 dark:text-gray-200"
                aria-labelledby="dropdownUserAvatarButton"
        >
        </ul>
    </div>
</div>

<nav>
    <div class="menu-items">
        <ul class="nav-links">
            <li>
                <a href="/" class="btn <% if(active === 'home'){ %>selected<% } %>">
                    <i class="ti-home"></i>
                    <span class="link-name">Tổng quan</span>
                </a>
            </li>
            <li>
                <a
                        href="/categories"
                        class="btn <% if(active === 'category'){ %>selected<% } %>"
                >
                    <i class="ti-layers"></i>
                    <span class="link-name">Danh mục</span>
                </a>
            </li>
            <li>
                <a class="btn <% if(active === 'product'){ %>selected<% } %>">
                    <i class="ti-package"></i>
                    <span class="link-name">Sản phẩm</span>
                </a>
            </li>
            <li>
                <a class="btn <% if(active === 'vendor'){ %>selected<% } %>">
                    <i class="ti-user"></i>
                    <span class="link-name">Nhà cung cấp</span>
                </a>
            </li>
            <li>
                <a class="btn <% if(active === 'statistical'){ %>selected<% } %>">
                    <i class="ti-bar-chart"></i>
                    <span class="link-name">Thống kê</span>
                </a>
            </li>
        </ul>

        <ul class="logout-mode">
            <li class="mode">
                <a href="#">
                    <i class="uil uil-moon"></i>
                    <span class="link-name">Giao diện</span>
                </a>

                <div class="mode-toggle">
                    <span class="switch"></span>
                </div>
            </li>

            <li><a href="#">
                    <i class="uil uil-signout"></i>
                    <span class="link-name">Đăng xuất</span>
                </a></li>
        </ul>
    </div>
</nav>
